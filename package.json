{
  "name": "motorrijtuigenbelasting",
  "version": "1.3.0",
  "repository": "github:hongaar/motorrijtuigenbelasting",
  "license": "MIT",
  "author": "Joram van den Boezem <joram@vandenboezem.nl>",
  "sideEffects": false,
  "type": "module",
  "main": "dist/index.js",
  "types": "types/index.d.ts",
  "files": [
    "dist",
    "types"
  ],
  "scripts": {
    "build": "yarn clean && tsc",
    "build:watch": "tsc --watch",
    "clean": "rm -rf dist && rm -rf types",
    "format": "prettier --write --ignore-unknown .",
    "format:check": "prettier --check --ignore-unknown .",
    "prepublish": "yarn build",
    "release": "semantic-release",
    "test": "jest",
    "test:watch": "jest --watch"
  },
  "lint-staged": {
    "*": "prettier --write --ignore-unknown"
  },
  "devDependencies": {
    "@semantic-release/changelog": "6.0.2",
    "@semantic-release/git": "10.0.1",
    "@types/jest": "29.2.6",
    "@types/node": "18.11.18",
    "@types/prettier": "^2",
    "@types/semantic-release": "^20",
    "husky": "8.0.3",
    "jest": "29.3.1",
    "lint-staged": "13.1.0",
    "moker": "0.10.3",
    "prettier": "2.8.3",
    "semantic-release": "20.0.4",
    "semantic-release-yarn": "0.3.1",
    "ts-jest": "29.0.5",
    "typescript": "4.9.4"
  },
  "packageManager": "yarn@3.3.1",
  "moker": {
    "plugins": [
      "typescript",
      "jest",
      "prettier",
      "github-actions",
      "semantic-release",
      "husky",
      "lint-staged",
      "dependabot"
    ]
  }
}
